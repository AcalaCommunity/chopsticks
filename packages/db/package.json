{
	"name": "@acala-network/chopsticks-db",
	"version": "0.9.1-3",
	"author": "Acala Developers <hello@acala.network>",
	"license": "Apache-2.0",
	"type": "module",
	"scripts": {
		"clean": "rm -rf dist tsconfig.tsbuildinfo",
		"build": "yarn clean && npx rollup -c && tsc -p ./tsconfig.json"
	},
	"dependencies": {
		"@acala-network/chopsticks-core": "workspace:*",
		"idb": "^7.1.1",
		"sqlite3": "^5.1.6",
		"typeorm": "^0.3.17"
	},
	"devDependencies": {
		"@swc/cli": "0.1.62",
		"@swc/core": "^1.3.96",
		"typescript": "^5.1.6"
	},
	"files": [
		"dist",
		"*.d.ts",
		"*.mjs"
	],
	"main": "./dist/index.mjs",
	"module": "./dist/index.mjs",
	"types": "./dist/index.d.ts",
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"require": "./dist/index.cjs",
			"import": "./dist/index.mjs"
		},
		"./*": {
			"types": "./dist/index.d.ts",
			"require": "./dist/index.cjs",
			"import": "./dist/index.mjs"
		},
		"./package.json": "./package.json",
		"./package.cjs.json": "./dist/cjs/package.json"
	}
}
