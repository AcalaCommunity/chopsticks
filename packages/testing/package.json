		"@types/chai": "^4.3.5",
		"typescript": "^5.1.6"
	},
	"files": [
		"lib"
	],
	"main": "./lib/index.js",
	"types": "./lib/index.d.ts",
	"exports": {
		".": {
			"types": "./lib/index.d.ts",
			"default": "./lib/index.js"
