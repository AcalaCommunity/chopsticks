	"license": "Apache-2.0",
	"scripts": {
		"clean": "rm -rf lib tsconfig.tsbuildinfo",
		"build": "tsc -p ./tsconfig.json"
	},
	"dependencies": {
		"@acala-network/chopsticks": "workspace:*"
	},
	"peerDependencies": {
		"vitest": "^0.34.0"
	},
