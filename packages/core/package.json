{
	"name": "@acala-network/chopsticks-core",
	"version": "0.9.1-3",
	"author": "Acala Developers <hello@acala.network>",
	"license": "Apache-2.0",
	"type": "module",
	"scripts": {
		"clean": "rm -rf dist tsconfig.tsbuildinfo tsconfig.tsbuildinfo",
		"build": "yarn clean && npx rollup -c && tsc -p tsconfig.json",
		"docs:prep": "typedoc"
	},
	"dependencies": {
		"@acala-network/chopsticks-executor": "workspace:*",
		"@polkadot/api": "^10.10.1",
		"@polkadot/util-crypto": "^12.5.1",
		"comlink": "^4.4.1",
		"eventemitter3": "^5.0.1",
		"lodash": "^4.17.21",
		"pino": "^8.16.1",
		"pino-pretty": "^10.2.0",
		"zod": "^3.22.3"
	},
	"devDependencies": {
		"@swc/cli": "0.1.62",
		"@swc/core": "^1.3.96",
		"@types/lodash": "^4.14.199",
		"typescript": "^5.1.6"
	},
	"files": [
		"dist",
		"*.d.ts",
		"*.mjs"
	],
	"main": "./dist/index.mjs",
	"module": "./dist/index.mjs",
	"types": "./dist/index.d.ts",
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"require": "./dist/index.cjs",
			"import": "./dist/index.mjs"
		},
		"./*": {
			"types": "./dist/index.d.ts",
			"require": "./dist/index.cjs",
			"import": "./dist/index.mjs"
		},
		"./package.json": "./package.json",
		"./package.cjs.json": "./dist/cjs/package.json"
	},
	"browser": {
		"./dist/node-worker.cjs": "./dist/browser-worker.cjs",
		"./dist/node-worker.mjs": "./dist/browser-worker.mjs"
	}
}
